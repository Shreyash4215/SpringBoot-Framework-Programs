<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xmlns:p="http://www.springframework.org/schema/p"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context.xsd">
 
<!-- ********************************************************* 
	***Spring Lifecycle Using Annotation ***
	@PostConstruct and @PreConstruct annotation are available till java 8 
	so we have add new dependenc in POM file
 ********************************************************* -->

<!-- *** By Default all annotation are disabled,so we have to enable all annotation *** -->
	<context:annotation-config></context:annotation-config>
	
	<bean class="com.practical3.Student" name="beanStudent">
		<property name="roll" value="1"/>
		<property name="name" value="ABCD"/>	
	</bean>
 
 </beans>